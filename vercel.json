{
  "buildCommand": "",
  "outputDirectory": ".",
  "framework": "static",
  "public": true,
  "regions": ["iad1"],
  "redirects": [
    {
      "source": "/index",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}
